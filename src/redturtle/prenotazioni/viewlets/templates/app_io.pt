<div tal:condition="view/available">
  <h1><!-- TODO --></h1>
  <table class="listing"
         style="overflow: scroll;display: block;"
  >
    <thead>
      <th>Codice Fiscale</th>
      <th>Titolo</th>
      <th>Messaggio</th>
      <th>ID Messaggio</th>
